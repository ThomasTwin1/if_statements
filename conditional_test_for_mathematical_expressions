# Conditional Test for Mathematical Expressions
age = 1
print(age > 0) # age greater than 0. Output: True

age = 15
print(age < 10) # age less than 10. Output: False

age = 20
print(age <= 21) # age less than or equal to 21. Output: True

age = 25
print(age >= 27) # age greater than or equal to 27. Output: False
